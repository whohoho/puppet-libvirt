{
  "name": "cirrax-libvirt",
  "version": "3.0.0",
  "author": "Cirrax GmbH",
  "license": "GPL-3.0",
  "summary": "Puppet module to install libvirt and create virtual domain configuration",
  "source": "https://github.com/cirrax/puppet-libvirt",
  "project_page": "https://github.com/cirrax/puppet-libvirt",
  "issues_url": "https://github.com/cirrax/puppet-libvirt/issues",
  "tags": [
    "libvirt",
    "kvm",
    "drbd",
    "virtualization"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "17.10",
        "18.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.0.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.0.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    }
  ]
}
